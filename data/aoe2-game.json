  "ruleset": {
    "name": "aoe2",
    "dslLines": [
      "# Some common 'rules' that simulate unit AI and make writing build orders more convenient",
      "after every completed lure_boar assign to boar",
      "after every completed lure_boar assign villager all from sheep to boar",
      "after every depleted boar assign to boar deer sheep",
      "after every depleted deer assign to boar deer sheep",
      "after every depleted sheep assign to boar deer sheep straggler_trees",
      "after every exhausted sheep assign to boar deer straggler_trees",
      "after every exhausted berries assign to straggler_trees",
      "after every depleted straggler_trees assign to straggler_trees",
      "after every completed build_farm assign to created",
      "after every depleted shore_fish assign to boar deer sheep berries straggler_trees",
      "after every depleted deep_fish assign to deep_fish shore_fish"
    ]
  },
  "settings": {
    "arabia": {
      "dslLines": [
        "start with town_center, villager x3, scout_cavalry",
        "starting-resource food 200",
        "starting-resource wood 200",
        "starting-resource gold 100",
        "starting-resource stone 200",
        "start-node starter_sheep 2",
        "start-node neutral_sheep 6",
        "start-node wild_boar 2",
        "start-node wild_deer 3",
        "start-node berries 1",
        "start-node shore_fish 4",
        "start-node deep_fish 6",
        "start-node forest 1",
        "start-node straggler_trees 5",
        "start-node gold_mine 1",
        "start-node stone_mine 1"
      ]
    },
    "normal_efficiency": {
      "dslLines": [
        "# to account for walking and various inefficiencies",
        "# sourced by vibes & SOTL. Stragglers estimated using his lumber camp vid",
        "modifier gather.rate.node.sheep mul 0.97",
        "modifier gather.rate.node.boar mul 0.95",
        "modifier gather.rate.node.deer mul 0.95",
        "modifier gather.rate.node.berries mul 0.96",
        "modifier gather.rate.node.shore_fish mul 0.95",
        "modifier gather.rate.node.deep_fish mul 0.9",
        "modifier gather.rate.node.forest mul 0.91",
        "modifier gather.rate.node.straggler_trees mul 0.75"
      ]
    },
    "conservative_efficiency": {
      "dslLines": [
        "# More conservative settings for resource efficiency, can be good if you're more of a beginner",
        "modifier gather.rate.node.sheep mul 0.92",
        "modifier gather.rate.node.boar mul 0.90",
        "modifier gather.rate.node.deer mul 0.90",
        "modifier gather.rate.node.berries mul 0.91",
        "modifier gather.rate.node.shore_fish mul 0.90",
        "modifier gather.rate.node.deep_fish mul 0.85",
        "modifier gather.rate.node.forest mul 0.86",
        "modifier gather.rate.node.straggler_trees mul 0.70"
      ]
    }
  },
